{
  "compilerOptions": {
    "jsx": "react",
    "allowJs": true,
    "target": "es6",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "moduleResolution": "node"
  },
  "include": ["./**/*"]
}
